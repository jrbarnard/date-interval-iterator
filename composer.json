{
    "name": "jrbarnard/date-interval-iterator",
    "description": "A package for creating a collection of dates based off a variety of regular intervals",
    "license": "MIT",
    "keywords": ["PHP","Datetime","Iterator","Date interval"],
    "homepage": "https://github.com/jrbarnard/date-interval-iterator",
    "authors": [
        {
            "name": "James Barnard",
            "email": "james.r.barnard@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phpdocumentor/phpdocumentor": "^2.8",
        "mockery/mockery": "^0.9.7",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "JRBarnard\\DateIntervalIterator\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test:cov": "phpunit --coverage-html ./tests/report",
        "test:style": "./vendor/bin/phpcs --standard=PSR2 src"
    }
}
